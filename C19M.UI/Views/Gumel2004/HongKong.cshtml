@model C19M.UI.Models.Gumel2004.HongKongModel
<div>
    <canvas id="HongKong_Canvas"></canvas>
</div>
<script>
    GenerateChartGumel(
        'HongKong_Canvas',
        300,
        1000,
        @Html.Raw(Json.Serialize(Model.Days)),
        @Html.Raw(Json.Serialize(Model.DayCumulativeDiseaseInducedDeaths_Array)),
        @Html.Raw(Json.Serialize(Model.DayAsymptomaticIndividuals_Array)),
        @Html.Raw(Json.Serialize(Model.DaySymptomaticIndividuals_Array)),
        @Html.Raw(Json.Serialize(Model.DayIsolatedIndividuals_Array)),
        @Html.Raw(Json.Serialize(Model.DayQuarantinedIndividuals_Array)),
        @Html.Raw(Json.Serialize(Model.DayRecoveredIndividuals_Array)),
        @Html.Raw(Json.Serialize(Model.DaySusceptibleIndividuals_Array)),
        'Day',
        'Number of Individuals',
        'Hong Kong'
    );
</script>